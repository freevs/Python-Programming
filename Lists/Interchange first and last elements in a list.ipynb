   "source": [
    "#  Interchange first and last elements in a list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "cd1783bb",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[24, 35, 9, 56, 12]\n"
     ]
    }
   ],
   "source": [
    "def swapList(list):\n",
    "    size = len(list)\n",
    "     \n",
    "    temp = list[0]\n",
    "    list[0] = list[size - 1]\n",
    "    list[size - 1] = temp\n",
    "     \n",
    "    return list\n",
    "\n",
    "list = [12, 35, 9, 56, 24]\n",
    "print(swapList(list))"
   ]
  }
 ],
